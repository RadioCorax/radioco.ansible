[buildout]
parts = radioco

extensions = mr.developer
allow-picked-versions = false
show-picked-versions = true
newest = false
versions = versions

sources = sources
auto-checkout = *
always-checkout = force

[sources]
radioco.frapp = git https://github.com/stefan-walluhn/radioco.frapp.git
radioco = git https://github.com/stefan-walluhn/django-radio.git rev={{ radioco_revision }}

[radioco]
recipe = djangorecipe
project = wrapper
eggs =
    radioco.frapp
    radioco
    psycopg2
    gunicorn

control-script = radioco-admin
scripts-with-settings = gunicorn
settings = settings

[versions]
Django = 1.8.18
Pillow = 4.1.1
django-bootstrap3 = 8.2.3
django-bower = 5.2.0
django-ckeditor = 5.2.2
django-disqus = 0.5
django-filebrowser = 3.6.4
django-filter = 1.0.2
django-grappelli = 2.7.3
django-recurrence = 1.4.1
django-solo-grappelli = 1.1.2
djangorecipe = 2.2.1
djangorestframework = 3.6.2
funcsigs = 1.0.2
gunicorn = 19.7.1
mock = 1.3.0
mr.developer = 1.34
olefile = 0.44
pbr = 1.8.1
psycopg2 = 2.7.1
python-dateutil = 2.6.0
pytz = 2017.2
six = 1.10.0
zc.recipe.egg = 2.0.3
