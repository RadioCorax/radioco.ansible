[buildout]
parts = radioco

extensions = mr.developer
allow-picked-versions = false
show-picked-versions = true
newest = false
versions = versions

sources = sources
auto-checkout = *
#always-checkout = force

[sources]
radioco = git https://github.com/stefan-walluhn/django-radio.git branch={{ branch }}

[radioco]
recipe = djangorecipe
project = radioco
eggs =
    radioco
    psycopg2
    gunicorn

control-script = radioco-admin
scripts-with-settings = gunicorn
settings = configs.common.settings

[versions]
Django = 1.8.4
Pillow = 3.2.0
django-bootstrap3 = 7.0.1
django-bower = 5.1.0
django-ckeditor = 5.0.3
django-disqus = 0.5
django-filebrowser = 3.6.4
django-filter = 0.13.0
django-grappelli = 2.7.3
django-recurrence = 1.2.0
djangorecipe = 2.1.2
djangorestframework = 3.3.3
funcsigs = 0.4
gunicorn = 19.4.5
mock = 1.3.0
mr.developer = 1.34
pbr = 1.8.1
psycopg2 = 2.6.1
python-dateutil = 2.5.2
pytz = 2016.3
six = 1.10.0
zc.recipe.egg = 2.0.3
