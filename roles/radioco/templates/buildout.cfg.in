[buildout]
parts = radioco

extensions = mr.developer
allow-picked-versions = false
show-picked-versions = true
newest = false
versions = versions

sources = sources
auto-checkout = *
always-checkout = force

[sources]
radioco.frapp = git https://github.com/RadioCorax/radioco.frapp.git
radioco = git https://github.com/RadioCorax/radioco.git rev={{ radioco_revision }}

[radioco]
recipe = djangorecipe
project = wrapper
eggs =
    radioco.frapp
    radioco
    psycopg2-binary
    gunicorn

control-script = radioco-admin
scripts-with-settings = gunicorn
settings = settings

[versions]
Django = 1.11.13
Pillow = 5.1.0
django-bootstrap3 = 10.0.1
django-ckeditor = 5.5.0
django-disqus = 0.5
django-filebrowser = 3.9.1
django-filter = 1.1.0
django-grappelli = 2.10.2
django-js-asset = 1.1.0
django-npm = 1.0.0
django-recurrence = 1.6.0
django-solo-grappelli = 1.1.2
djangorecipe = 2.2.1
djangorestframework = 3.8.2
funcsigs = 1.0.2
gunicorn = 19.7.1
mock = 2.0.0
mr.developer = 1.38
olefile = 0.45.1
pbr = 4.0.2
psycopg2-binary = 2.7.4
python-dateutil = 2.7.3
pytz = 2018.4
six = 1.11.0
zc.recipe.egg = 2.0.5
